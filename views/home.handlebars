<div class="container">
    <div class="row">
        <nav aria-label="breadcrumb">
        <ol class="breadcrumb">
            <li class="breadcrumb-item active" aria-current="page">Calculo de Frete por KM</li>
        </ol>
        </nav>
    </div>
    <div class="row">
        <div class="col">
            <p>
                <a class="btn btn-primary" data-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">
                    Coloque seu endereço de origem
                </a>
                <a class="btn btn-primary" data-toggle="collapse" href="#collapseExample2" role="button" aria-expanded="false" aria-controls="collapseExample">
                    Coloque seu endereço de destino
                </a>
                <form action="/geolocaliza" method="POST" class="form" role="form" autocomplete="off">
                <div class="col-lg-3">
                    <input class="form-control" type="text" name="frete" id="frete" placeholder="Valor do Frete por KM">
                </div>
            </p>
        </div>
    </div>
    <div class="row">
        <div class="col">
            <div class="collapse multi-collapse" id="collapseExample">
                <div class="card card-body">
                        <!-- form user info -->
                        <div class="card card-outline-secondary">
                            <div class="card-header">
                                <h3 class="mb-0">Coloque seu endereço de origem:</h3>
                            </div>
                            <div class="card-body">
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Via</label>
                                        <div class="col-lg-9">
                                            <select id="user_time_zone" name="viaOrigem" class="form-control" size="0">
                                                <option value="Avenida" selected="selected">Avenida</option>
                                                <option value="Rua">Rua</option>
                                                <option value="Largo">Largo</option>
                                                <option value="Praça">Praça</option>
                                                <option value="Morro">Morro</option>
                                                <option value="Viela">Viela</option>
                                                <option value="Parque">Parque</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Logradouro</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="logradouroOrigem" value="Saturnino de Brito">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Número</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="numeroOrigem">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Bairro</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="bairroOrigem">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Cidade e/ou Estado</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="cidadeOrigem">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">País</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="paisOrigem">
                                        </div>
                                    </div>
                            </div>
                        </div>
                        <!-- /form user info -->
                </div>
            </div>
        </div>
        <div class="col">
            <div class="collapse multi-collapse" id="collapseExample2">
                <div class="card card-body">
                        <!-- form user info -->
                        <div class="card card-outline-secondary">
                            <div class="card-header">
                                <h3 class="mb-0">Coloque seu endereço de destino:</h3>
                            </div>
                            <div class="card-body">
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Via</label>
                                        <div class="col-lg-9">
                                            <select id="user_time_zone" name="viaDestino" class="form-control" size="0">
                                                <option value="Avenida" selected="selected">Avenida</option>
                                                <option value="Rua">Rua</option>
                                                <option value="Largo">Largo</option>
                                                <option value="Praça">Praça</option>
                                                <option value="Morro">Morro</option>
                                                <option value="Viela">Viela</option>
                                                <option value="Parque">Parque</option>
                                            </select>
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Logradouro</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="logradouroDestino" value="Saturnino de Brito">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Número</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="numeroDestino">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Bairro</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="bairroDestino">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">Cidade e/ou Estado</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="cidadeDestino">
                                        </div>
                                    </div>
                                    <div class="form-group row">
                                        <label class="col-lg-3 col-form-label form-control-label">País</label>
                                        <div class="col-lg-9">
                                            <input class="form-control" type="text" name="paisDestino">
                                        </div>
                                    </div>
                            </div>
                        </div>
                        <!-- /form user info -->
                    
                </div>
            </div>
        </div>
        <input type="submit" class="btn btn-primary" value="Verificar" style="margin-top: 10px;">
        </form>
    </div>
</div>